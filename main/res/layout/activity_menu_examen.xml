<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewMenuExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



        <TextView
            android:id="@+id/txtLibRessourcePlanning"
            style="?attr/textLibRessourcePlanningMenuExamen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/btnPlanningExamen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="129dp"
            android:text="@string/btnDownloadPlanningExamen" />

        <TextView
            android:id="@+id/txtLibRessourceResultat"
            style="?attr/textLibRessourceExamenMenuExamen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btnDownloadResultatExamen"
            android:id="@+id/btnResultat"
            android:layout_row="7"
            android:layout_column="2"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />


</RelativeLayout>