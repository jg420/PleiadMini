<resources id="R">
    <color name="custom_theme_color">#b0b0ff</color>

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <!-- Base application theme. -->
    <style name="CustomTheme" parent="Theme.AppCompat.Light.NoActionBar">
         <!--<item name="android:background">#b0b0ff</item>-->
        <!-- Customize your theme here. -->
        <item name="buttonValidLogin">@style/btnValidLogin</item>
        <item name="buttonQuitApplication">@style/buttonQuitApplication</item>
        <item name="buttonDownloadPlanningCours">@style/buttonDownloadPlanningCours</item>
        <item name="buttonDownloadPlanningExamen">@style/buttonDownloadPlanningExamen</item>
        <item name="buttonDownloadResultatExamen">@style/buttonDownloadResultatExamen</item>
        <item name="buttonContactDev">@style/buttonContactDev</item>
        <item name="buttonRetour">@style/buttonRetour</item>
        <item name="textTitlePageConnexion">@style/textTitlePageConnexion</item>
        <item name="textLibLoginMenuConnexion">@style/textLibLoginMenuConnexion</item>
        <item name="textLibMdpMenuConnexion">@style/textLibMdpMenuConnexion</item>
        <item name="textInfoConnexionMenuConnexion">@style/textInfoConnexionMenuConnexion</item>
        <item name="textInfoCoursMenuCour">@style/textInfoCoursMenuCour</item>
        <item name="textChargement">@style/textChargement</item>
        <item name="textInfoMenuDivers">@style/textInfoMenuDivers</item>
        <item name="textLibRessourcePlanningMenuExamen">@style/textLibRessourcePlanningMenuExamen</item>
        <item name="textLibRessourceExamenMenuExamen">@style/textLibRessourceExamenMenuExamen</item>
        <item name="editTextLogin">@style/editTextLogin</item>
        <item name="editTextMdp">@style/editTextMdp</item>
        <item name="linearLayoutMenu">@style/linearLayoutMenu</item>
        <item name="linearLayoutMenuCours">@style/linearLayoutMenuCours</item>
        <item name="linearLayoutMenuCoursDetail">@style/linearLayoutMenuCoursDetail</item>
        <item name="linearLayoutMenuDivers">@style/linearLayoutMenuDivers</item>
        <item name="linearLayoutMenuExamen">@style/linearLayoutMenuExamen</item>
        <item name="switchOnline">@style/switchOnline</item>
        <item name="listViewSeance">@style/listViewSeance</item>
        <item name="listViewSeances">@style/listViewSeances</item>
        <item name="expandableListViewDetailCours">@style/expandableListViewDetailCours</item>
        <item name="tabWidgetMenu">@style/tabWidgetMenu</item>
        <item name="textTitlePageMenuPrincipal">@style/textTitlePageMenuPrincipal</item>
    </style>

    <style name="btnValidLogin">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/btnValidConnexion</item>


    </style>
    <style name="buttonQuitApplication">
        <item name="android:layout_width">10mm</item>
        <item name="android:layout_height">10mm</item>
        <!--<item name="android:text">@string/btnQuitter</item>-->
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@drawable/exit</item>
  <!--      <item name="android:layout_alignParentEnd">true</item>-->
    </style>
    <style name="buttonDownloadPlanningCours">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:enabled">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentRight">true</item>
      <!--  <item name="android:layout_alignParentEnd">true</item>-->
        <item name="android:text">@string/btnDownloadPlanningCours</item>
    </style>
    <style name="buttonDownloadPlanningExamen">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/btnDownloadPlanningExamen</item>
        <item name="android:layout_row">7</item>
        <item name="android:layout_column">0</item>
    </style>
    <style name="buttonDownloadResultatExamen"></style>
    <style name="buttonContactDev">
        <item name="android:layout_width">10mm</item>
        <item name="android:layout_height">10mm</item>
        <!--<item name="android:text">@string/btnContactDev</item>-->
        <item name="android:background">@drawable/email_button</item>
        <item name="android:layout_gravity">right</item>
    </style>
    <style name="buttonRetour">
        <item name="android:layout_width">15mm</item>
        <item name="android:layout_height">6mm</item>
        <item name="android:text">@string/btnRetourCours</item>
        <item name="android:background">@drawable/boutonretour</item>
        <item name="android:layout_gravity">right</item>

    </style>

    <!--<style name=""></style>-->
    <style name="tableLayout_MenuExamen">
        <item name="android:layout_width">197dp</item>>
        <item name="android:layout_height">207dp</item>>
        <item name="android:layout_gravity">center_horizontal</item>>
        <item name="android:layout_weight">0.25</item>>
    </style>
    <style name="tabWidgetMenu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">#ffff0000</item>

    </style>
    <style name="textTitlePageMenuPrincipal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#b60d1493</item>

        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="textTitlePageConnexion">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:text">@string/txtTitleConnectPage</item>
        <item name="android:textColor">#b60d1493</item>
        <item name="android:textSize">25sp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignLeft">@+id/textEtatConnexion</item>

        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="textLibLoginMenuConnexion">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:text">@string/lib_Login</item>
        <item name="android:layout_marginLeft">5dp</item>


    </style>

    <style name="textLibMdpMenuConnexion">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:text">@string/lib_Mdp</item>
        <item name="android:layout_marginLeft">5dp</item>
      <!--  <item name="android:layout_marginLeft">35dp</item>-->
      <!--  <item name="android:layout_above">@+id/btnValidLogin</item>-->
       <!-- <item name="layout_toLeftOf">@+id/textViewTitle</item>
        <item name="layout_toStartOf">@+id/textViewTitle</item>-->
    </style>

    <style name="textInfoConnexionMenuConnexion">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:text">@string/txtEtatConnexion</item>
        <item name="android:textColor">#e3d40100</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginBottom">72dp</item>
    </style>

    <style name="textInfoCoursMenuCour">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#b6000000</item>

        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:text">@string/txtResumeListCour</item>
        <item name="android:layout_row">0</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_toLeftOf">@+id/btnDownloadPlanningCours</item>
        <item name="android:layout_alignBottom">@+id/btnDownloadPlanningCours</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="textDetailListSeance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/libDetailsCours</item>
        <item name="android:textColor">#b6000000</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="textChargement"></style>

    <style name="textInfoTelechargementMenuExamen">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">string/txtMenuExamen</item>
        <item name="android:layout_row">0</item>
        <item name="android:layout_column">0</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold|normal</item>
        <!--<item name="android:textColor">#ffff0518</item>-->
    </style>

    <style name="textInfoMenuDivers">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/txtInfoMenuDivers</item>
    </style>
    .
    <style name="textLibRessourcePlanningMenuExamen">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:text">@string/txtIntituleRessourcePlanning</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="textLibRessourceExamenMenuExamen">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:text">@string/txtIntituleRessourcePlanning</item>
        <item name="android:textSize">20dp</item>
    </style>


    <style name="editTextLogin">

        <!--<item></item>-->
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>



    </style>
    <style name="editTextMdp">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
       

    </style>
    <style name="linearLayoutMenu">
        <item name="android:weightSum">1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:rowCount">1</item>
        <item name="android:columnCount">3</item>
        <item name="android:orientation">vertical</item>
      <item name="android:background">#92c1faff</item>
    </style>

    <style name="linearLayoutConnexion">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>



    </style>

    <style name="linearLayoutMenuCours">

        <item name="android:weightSum">1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:rowCount">1</item>
        <item name="android:columnCount">3</item>
    </style>
    <style name="linearLayoutMenuCoursDetail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="linearLayoutMenuDivers">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="linearLayoutMenuExamen">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

    </style>
    <style name="switchOnline">
        <item name="android:text">@string/swcLogOff</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:checked">true</item>

    </style>

    <style name="listViewSeance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">false</item>
     <!--   <item name="android:textColor">#e32967d4</item>-->
        <item name="android:textFilterEnabled">false</item>
        <item name="android:layout_below">@+id/btnDownloadPlanningCours</item>
    <!--    <item name="android:background">#92c1faff</item>-->

    </style>
    <style name="listViewSeances"></style>
    <style name="expandableListViewDetailCours">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHighlight">#bdfbee1c</item>
        <item name="android:textColor">#ff000000</item>
    </style>



</resources>
